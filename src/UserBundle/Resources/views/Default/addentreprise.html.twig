{% extends 'back.html.twig' %}
{% block body %}
    {% form_theme form 'bootstrap_3_layout.html.twig' %}
    {{ form(form) }}
    {% for flashMessage in app.session.flashBag.get('success') %}
        <div class="alert alert-success">
            {{ flashMessage }}
        </div>
    {% endfor %}
    {{ form_errors(form) }}
{% endblock %}